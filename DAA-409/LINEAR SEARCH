
#include<stdio.h>

int main()
{
    int n,key;
        printf("enter number of elements:");
        scanf("%d",&n);
        int a[n];
        for(int i=0;i<n;i++)
        {
            scanf("%d",&a[i]);
        }

        printf("enter key:");
        scanf("%d",&key);

        for(int i=0;i<n;i++)
        {


            if (a[i]==key)
                {
        printf("key found \n");
        break;
                }
        }

    return 0;
}
